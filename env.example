# Environment Configuration
# Copy this file to .env.local, .env.development, .env.staging, or .env.production
# and update the values according to your environment
#
# Note: NODE_ENV is automatically set by npm scripts and should not be included in these files

# Environment
NEXT_PUBLIC_ENV=development

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3010/api
NEXT_PUBLIC_API_TIMEOUT=10000

# Feature Flags
NEXT_PUBLIC_SHOW_LOGGER=true
NEXT_PUBLIC_SHOW_DEV_TOOLS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Authentication
NEXT_PUBLIC_AUTH_DOMAIN=your-auth-domain.auth0.com
NEXT_PUBLIC_AUTH_CLIENT_ID=your-client-id

# External Services
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=

# Development Tools
NEXT_PUBLIC_ENABLE_MOCK_API=false
NEXT_PUBLIC_MOCK_API_DELAY=1000

# Performance
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=false
NEXT_PUBLIC_ENABLE_BUNDLE_ANALYZER=false

# AI Services
NEXT_PUBLIC_COHERE_API_KEY=
NEXT_PUBLIC_ENABLE_AI_SUPPORT=true 